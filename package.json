{
  "name": "svelte-tauri-filedrop",
  "version": "0.0.0",
  "description": "Tauri file drop handling component for Svelte",
  "type": "module",
  "scripts": {
    "dev": "tauri dev",
    "dev:web": "svelte-kit dev",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --write --plugin-search-dir=. src *.js && eslint --fix src *.js"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.0-next.201",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.0",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.44.0",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.10",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/probablykasper/svelte-tauri-filedrop.git"
  },
  "bugs": {
    "url": "https://github.com/probablykasper/svelte-tauri-filedrop/issues"
  },
  "author": {
    "name": "Kasper Henningsen",
    "url": "https://kasper.space"
  },
  "keywords": [
    "tauri",
    "svelte",
    "file",
    "drag",
    "drop",
    "dropzone"
  ],
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "strict",
    "svelteSortOrder": "options-scripts-markup-styles",
    "svelteStrictMode": false,
    "svelteAllowShorthand": true,
    "svelteIndentScriptAndStyle": true
  }
}
